{
  "manifest_version": 2,

  "name": "Research Blocks",
  "description": "Allows users to conveniently store links to webpages and images while doing research",
  "version": "1.0",

  "permissions": [
	"contextMenus",
	"tabs"
  ],
  "icons":{
	"16": "rb_icon.png"
  },
  
  "background": {
    "scripts": ["test.js"]
  },
  
  "browser_action": {
    "default_icon": "rb_icon.png",
    "default_popup": "test1.html"
  }
}